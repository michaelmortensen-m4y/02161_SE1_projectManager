<GridPane fx:id="editActivityLimitWindow" prefHeight="600.0" prefWidth="600.0" vgap="10" alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.EditActivityLimitMenuController">
  <TextField fx:id="limitField" text="Limit" prefWidth="120" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="2">
    <GridPane.margin><Insets left="10.0" /></GridPane.margin>
  </TextField>
  <Button fx:id="setButton" mnemonicParsing="false" text="Set" prefWidth="60" onMouseClicked="#editActivityLimitMouseClicked" GridPane.columnIndex="0" GridPane.rowIndex="1">
    <GridPane.margin><Insets left="10.0" /></GridPane.margin>
  </Button>
  <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" prefWidth="60" onMouseClicked="#cancelMouseClick" GridPane.columnIndex="1" GridPane.rowIndex="1">
    <GridPane.margin><Insets left="10.0" /></GridPane.margin>
  </Button>
