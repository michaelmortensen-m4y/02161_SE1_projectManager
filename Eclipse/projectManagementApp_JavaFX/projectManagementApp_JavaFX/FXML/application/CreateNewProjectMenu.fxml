<GridPane fx:id="createNewProjectWindow" prefHeight="600.0" prefWidth="600.0" alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CreateNewProjectMenuController">
    <TextField fx:id="newProjectNameField" text="Project name" prefWidth="130" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="2">
      <GridPane.margin><Insets left="10.0" right="10.0" bottom="10.0" top="10.0" /></GridPane.margin>
    </TextField>
    <Button fx:id="createButton" mnemonicParsing="false" text="Create" prefWidth="60" onMouseClicked="#createNewProjectMouseClicked" GridPane.columnIndex="0" GridPane.rowIndex="1">
      <GridPane.margin><Insets left="10.0" right="10.0" bottom="10.0" top="2.0" /></GridPane.margin>
    </Button>
    <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" prefWidth="60" onMouseClicked="#cancelMouseClicked" GridPane.columnIndex="1" GridPane.rowIndex="1">
      <GridPane.margin><Insets left="10.0" right="10.0" bottom="10.0" top="2.0" /></GridPane.margin>
    </Button>
</GridPane>

